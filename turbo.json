{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "SUPABASE_SERVICE_ROLE_KEY",
        "DATABASE_URL",
        "STRIPE_SECRET_KEY",
        "OWNER_EMAILS"
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "@cleanly/db#build": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
